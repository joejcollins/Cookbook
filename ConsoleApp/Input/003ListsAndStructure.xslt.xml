        Stir in
        <Grocery Quantity="400" Unit="g" Type="canned chick peas" Calories="400canned chick peasg" Fat="1.5" Process="drained" />,
        <Grocery Quantity="75" Unit="g" Calories="75raisinsg" Fat="1.5" Type="raisins" />,
        <Vegetable Quantity="1000" Unit="g" Type="sweet potatoes" Calories="1000sweet potatoesg" Fat="1.5" Process="sliced" />,
        <Grocery Quantity="400" Unit="g" Type="canned tomatoes" Calories="400canned tomatoesg" Fat="1.5" Process="chopped" />
