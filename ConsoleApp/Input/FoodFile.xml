<?xml version="1.0" encoding="utf-8" ?>
<Book xmlns="http://www.joejcollins.co.uk/CookBook/CookML.xsd">
  
  <Menu Title="Test Menu">
    <Recipe Title="Meat stew and dumplings" Meals="2" >
      <Index>
        Stew<Sub>Meat and dumplings</Sub>
      </Index>
      <Index>Dumplings</Index>
      <Stage>
        <Staple Name="potatoes" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="320" Type="onions" Unit="g" />,
        <Meat Process="sliced" Quantity="400" Type="meat" Unit="g" />,
        <Vegetable Process="chopped" Quantity="400" Type="carrots" Unit="g" />
        and
        <Vegetable Process="chopped" Quantity="400" Type="celery" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until meat done.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="75" Type="tomato puree" Unit="g"/>,
        <Check Quantity="0.5" Type="chilli powder" Unit="tsp" />,
        <Check Process="crumbled" Quantity="1" Type="stock cube" />,
        <Water Quantity="500" Type="cold water" Unit="ml" />
        and simmer for 50 minutes.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl" /> mix
        <Grocery Quantity="100" Unit="g" Type="self raising flour" />,
        <Grocery Quantity="75" Type="suet" Unit="g" />,
        <Check Quantity="0.5" Type="mixed herbs" Unit="tsp" />
        and
        <Water Quantity="80" Type="cold water" Unit="ml" />
        and knead into 8 balls.
        Put balls in stew, cover and
        simmer for 15 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Onion and tomato lasagna"  Meals="2">
      <Index>
        Lasagna<Sub>Onion and tomato</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="700" Type="onions" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until soft.
      </Stage>
      <Stage>
        Stir in <Grocery Process="chopped" Quantity="400" Type="canned tomatoes" Unit="g" />
        and warm through.
      </Stage>
      <Stage>
        In a <Utensil Name="casserole" /> layer
        <Grocery Quantity="200" Type="lasagna" Unit="g" />,
        the tomato mix and
        <Dairy Process="grated" Quantity="150" Type="cheese" Unit="g" />
        3 times.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl"/> mix
        <Dairy Quantity="300" Type="soured cream" Unit="ml" />
        with
        <Dairy Quantity="100" Type="milk" Unit="ml" />
        and pour on top of the lasagna.
      </Stage>
      <Stage>
        Bake <Temperature Level="medium" /> 50 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Chicken with cabbage and peanuts" Meals="2">
      <Stage>
        <Staple Name="noodles" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" />	fry
        <Grocery Quantity="70" Type="peanuts" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        then remove.
      </Stage>
      <Stage>
        Fry <Vegetable Process="chopped" Quantity="2" Type="red chilli" />
        and
        <Meat Process="sliced" Quantity="300" Type="chicken" Unit="g" />
        in the oil until meat is done.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Process="sliced" Quantity="750" Type="cabbage" Unit="g" />
        and heat through.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Type="black bean sauce" Unit="tbsp" />
        and fry for 2 minutes.
      </Stage>
      <Stage>
        Sprinkle with the peanuts.
      </Stage>
    </Recipe>
    <Recipe Title="Beans and fried eggs in pitta" Meals="1">
      <Stage>
        In a <Utensil Name="pan" /> warm
        <Grocery Quantity="400" Type="baked beans" Unit="g" />.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Dairy Quantity="2" Type="eggs" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
      </Stage>
      <Stage>
        Grill
        <Grocery Quantity="4" Type="pitta bread" />
      </Stage>
      <Stage>
        Serve with <Vegetable Quantity="200" Unit="g" Type="tomatoes" Process="sliced"/>.
      </Stage>
    </Recipe>
    <Recipe Title="Egg and vegetable curry" Meals="2">
      <Index>Curry</Index>
      <Stage>
        In <Utensil Name="pan" /> boil
        <Dairy Quantity="4" Type="eggs" />
      </Stage>
      <Stage>
        <Staple Name="rice"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="350" Type="onions" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until soft.
      </Stage>
      <Stage>
        Stir in <Check Quantity="1" Type="chilli powder" Unit="tbsp" />
        and
        <Check Quantity="2" Type="mustard seed" Unit="tsp" />
        and fry until mustard seeds start to pop.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="2" Unit="tbsp" Type="flour" />
        until it is all absorbed.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="150" Unit="g" Type="tomato puree"/>,
        <Grocery Quantity="50" Unit="g" Type="creamed coconut" Process="crumbled"/>
        and
        <Water Quantity="400" Unit="ml" Type="water" />
        and warm through.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="400" Unit="g" Type="mange tout" Process="halved"/>
        and the eggs
        and simmer for 10 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Lentil shepherds pie" Meals="2">
      <Index>
        Shepherds Pie<Sub>Lentil</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="pan"/> boil
        <Vegetable Quantity="1000" Unit="g" Type="boiling potatoes" Process="chopped"/>
        then mash.
      </Stage>
      <Stage>
        In a <Utensil Name="wok"/> fry
        <Vegetable Quantity="700" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>,
        <Vegetable Quantity="400" Unit="g" Type="celery" Process="chopped"/>,
        and
        <Vegetable Quantity="200" Unit="g" Type="carrots" Process="sliced"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="oil"/>
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="250" Unit="g" Type="green lentils" />,
        <Check Quantity="3" Unit="tsp" Type="miso" />,
        <Check Quantity="1" Type="stock cube" Process="crumbled" />,
        <Grocery Quantity="75" Unit="g" Type="tomato puree" />,
        <Water Quantity="650" Unit="ml" Type="boiling water" />,
        <Check Quantity="1" Unit="tsp" Type="marjoram" />,
        <Check Quantity="1" Unit="tsp" Type="ground cumin" />
        and
        <Check Quantity="2" Unit="tsp" Type="ground coriander" />
        then bring to boil,
        then simmer for 20 minutes.
      </Stage>
      <Stage>
        Take off heat,
        put in a <Utensil Name="casserole"/>,
        cover with the mashed potatoes
        and sprinkle with
        <Grocery Quantity="50" Unit="g" Type="sunflower seeds"/>.
        then bake <Temperature Level="medium"/> for 20 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Chickpea risotto" Meals="2">
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="400" Type="onions" Unit="g" />,
        <Vegetable Quantity="1" Type="red chilli" Process="chopped"/>
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="oil"/>
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="1000" Unit="g" Type="sweet potatoes" Process="sliced" />,
        <Grocery Quantity="400" Unit="g" Type="canned chick peas" Process ="drained"/>,
        <Grocery Quantity="400" Unit="g" Type="canned tomatoes" Process="chopped"/>,
        <Grocery Quantity="300" Unit="ml" Type="sherry"/>,
        <Grocery Quantity="75" Unit="g" Type="raisins" />,
        <Check Quantity="1" Type="stock cube" Process="crumbled" />,
        <Grocery Quantity="400" Unit="g" Type="white rice"/>
        and
        <Water Quantity="500" Unit="ml" Type="water" />
        and bring to a boil.
      </Stage>
      <Stage>
        Put in a
        <Utensil Name="casserole"/>,
        cover and bake <Temperature Level="low"/> 20 minutes.
      </Stage>
    <Stage>
      Server with 
      <Dairy Quantity="150" Unit="g" Type="parmesan" Process="grated"/>.
    </Stage>
    </Recipe>
    <Recipe Title="Tortellini and parmesan cheese" Meals="1">
      <Stage>
        <Staple Name="tortellini" />
      </Stage>
      <Stage>
        Sprinkle with
        <Grocery Quantity="20" Unit="g" Type="grated parmesan"/>.
      </Stage>
    </Recipe>
  </Menu>

  <Menu Title="Approved">
    <Recipe Title="Aubergine and peppers" Meals="2">
      <Stage>
        <Staple Name="potatoes" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="sliced"/>
        and
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="150" Unit="g" Type="red pepper" Process="sliced"/>,
        <Vegetable Quantity="150" Unit="g" Type="green pepper" Process="sliced"/>,
        <Vegetable Quantity="400" Unit="g" Type="aubergine" Process="sliced"/>,
        <Vegetable Quantity="400" Unit="g" Type="courgette" Process="sliced"/>,
        <Grocery Quantity="150" Unit="g" Type="tomato puree" />,
        <Water Quantity="100" Type="cold water" Unit="ml" />
        and
        <Vegetable Quantity="400" Unit="g" Type="tomatoes" Process="chopped"/>,
        and bring to the boil.
      </Stage>
      <Stage>
        Put in a <Utensil Name="casserole" /> and bake <Temperature Level="medium" /> for 40 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Potatoes Russian style" Meals="2">
      <Index>
        Style<Sub>Russian potatoes</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="pan" /> boil
        <Vegetable Quantity="3000" Unit="g" Type="boiling potatoes" Process="cubed"/>
        until not quite done
        then drain.
      </Stage>
      <Stage>
        Stir in
        <Dairy Quantity="1" Unit="large carton" Type="cottage cheese" />,
        <Dairy Quantity="300" Unit="ml" Type="sour cream" />,
        <Dairy Quantity="150" Unit="g" Type="cheese" Process="grated"/>
        and
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>.
      </Stage>
      <Stage>
        Put in <Utensil Name="casserole" /> and bake <Temperature Level="medium" /> for 1 hour.
      </Stage>
    </Recipe>
    <Recipe Title="Lentils and spicy sausage" Meals="2">
      <Stage>
        <Staple Name="rice brown" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        and
        <Meat Quantity="100" Unit="g" Type="bacon" Process="chopped"/>
        in
        <Check Quantity="3" Unit="tbsp" Type="oil" />
        until bacon done.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="250" Unit="g" Type="red lentils" />,
        <Grocery Quantity="2*400" Unit="g" Type="canned tomatoes" />,
        <Meat Quantity="250" Unit="g" Type="chorizo sausage" Process="sliced"/>,
        <Check Quantity="1" Unit="tsp" Type="chilli powder" />
        and
        <Water Quantity="400" Type="cold water" Unit="ml" />
        and simmer for 25 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Pizza and Swedish salad" Meals="1">
      <Index>
        Pizza
      </Index>
      <Stage>
        Bake <Grocery Quantity="2*9" Unit="inch" Type="pizza" />
        <Temperature Level="high" /> 15 minutes.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl"/> mix
        <Vegetable Quantity="250" Type="cabbage" Unit="g" Process="sliced" />,
        <Vegetable Quantity="50" Type="carrots" Unit="g" Process="sliced" />
        and
        <Vegetable Quantity="50" Type="onions" Unit="g" Process="sliced"/>.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Type="sugar" Unit="tbsp" />,
        <Check Quantity="2" Type="wine vinegar" Unit="tbsp" />,
        <Check Quantity="2" Type="oil" Unit="tbsp" />,
        <Check Quantity="1" Type="basil" Unit="pinch"/>,
        <Check Quantity="1" Type="oregano" Unit="pinch"/>,
        <Check Quantity="1" Type="salt" Unit="pinch"/>
        and
        <Check Quantity="1" Type="pepper" Unit="pinch"/>.
      </Stage>
    </Recipe>
    <Recipe Title="Mushroom Stew" Meals="2">
      <Index>
        Stew<Sub>Mushroom</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="375" Unit="g" Type="onions" Process="chopped"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="600" Unit="g" Type="mushrooms" />
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        and cook until mushrooms start to wilt.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="150" Unit="g" Type="pearl barley" />,
        <Check Quantity="4" Unit="tbsp" Type="soy sauce" />,
        <Check Quantity="5" Unit="tbsp" Type="sherry" />,
        <Check Quantity="1" Type="stock cube" Process="crumbled"/>
        and
        <Water Quantity="450" Type="cold water" Unit="ml" />
        and simmer for 25 minutes.
      </Stage>
      <Stage>
        <Staple Name="couscous"/>
      </Stage>
    </Recipe>
    <Recipe Title="Beans Caribbean style" Meals="2">
      <Index>
        Style<Sub>Carribbean beans</Sub>
      </Index>
      <Stage>
        <Staple Name="rice white"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry 
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="150" Unit="g" Type="green pepper" Process="chopped"/>,
        <Vegetable Quantity="1" Type="red chilli" Process="chopped"/>
        and
        <Vegetable Quantity="3" Unit="cloves" Type="garlic" Process="crushed"/>
        in 
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="400" Unit="g" Type="canned kidney beans" />
        and
        <Grocery Quantity="400" Unit="g" Type="canned chick peas" />
        in their liquid,
        cover and simmer for 5 minutes.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Unit="tsp" Type="dried thyme"/>,
        <Check Process="crumbled" Quantity="1" Type="stock cube" />
        and 
        <Grocery Quantity="400" Unit="g" Type="canned coconut milk" />
        Cover and simmer for 10 minutes.
      </Stage>
      <Stage>
        Stir in the rice and serve.
      </Stage>
    </Recipe>
    <Recipe Title="Tomatoes Indian style" Meals="2">
      <Index>
        Style<Sub>Indian tomatoes</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> heat
        <Check Quantity="4" Unit="tbsp" Type="oil" />,
        <Check Quantity="0.5" Unit="tsp" Type="asafoetida" />,
        <Check Quantity="0.5" Unit="tsp" Type="turmeric" />,
        and
        <Check Quantity="2" Unit="tsp" Type="cumin seeds" />
        until
        the cumin sizzles.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="1" Type="red chilli" Process="chopped"/>
        and
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="chopped"/>
        and
        fry until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="1000" Unit="g" Type="tomatoes" Process="chopped"/>
        and
        <Vegetable Quantity="400" Unit="g" Type="green beans" />
        simmer for 15 minutes.
      </Stage>
      <Stage>
        Serve with 
        <Dairy Quantity="500" Unit="g" Type="plain yoghurt"/>
        and 
        <Grocery Quantity="4" Type="naan bread"/>.
      </Stage>
    </Recipe>
    <Recipe Title="Bacon and mushroom pasta" Meals="1">
      <Stage>
        <Staple Name="tagliatelli"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry <Meat Quantity="150" Unit="g" Type="bacon" Process="chopped"/>
        and
        <Vegetable Quantity="200" Unit="g" Type="mushrooms" Process="sliced"/>
        in
        <Check Quantity="1" Unit="tbsp" Type="oil" /> for 5 minutes.
      </Stage>
      <Stage>
        Stir in the pasta (cooked and drained)
        and <Dairy Quantity="100" Unit="g" Type="cheese" Process="grated"/>
        and warm through.
      </Stage>
    </Recipe>
  </Menu>


</Book>