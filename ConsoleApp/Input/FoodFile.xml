<?xml version="1.0" encoding="utf-8" ?>
<Book xmlns="http://www.joejcollins.co.uk/CookBook/CookML.xsd">

  <Menu Title="Test Menu">
    <Recipe Title="Meat stew and dumplings" Meals="2" >
      <Index>
        Stew<Sub>Meat and dumplings</Sub>
      </Index>
      <Index>Dumplings</Index>
      <Stage>
        <Staple Name="potatoes" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="320" Type="onions" Unit="g" />,
        <Meat Process="sliced" Quantity="400" Type="meat" Unit="g" />,
        <Vegetable Process="chopped" Quantity="400" Type="carrots" Unit="g" />
        and
        <Vegetable Process="chopped" Quantity="400" Type="celery" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until meat done.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="75" Type="tomato puree" Unit="g"/>,
        <Check Quantity="0.5" Type="chilli powder" Unit="tsp" />,
        <Check Process="crumbled" Unit="cube" Quantity="1" Type="stock" />,
        <Water Quantity="500" Type="cold water" Unit="ml" />
        and simmer for 50 minutes.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl" /> mix
        <Grocery Quantity="100" Unit="g" Type="self raising flour" />,
        <Grocery Quantity="75" Type="suet" Unit="g" />,
        <Check Quantity="0.5" Type="mixed herbs" Unit="tsp" />
        and
        <Water Quantity="80" Type="cold water" Unit="ml" />
        and knead into 8 balls.
        Put balls in stew, cover and
        simmer for 15 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Onion and tomato lasagna"  Meals="2">
      <Index>
        Lasagna<Sub>Onion and tomato</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="700" Type="onions" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until soft.
      </Stage>
      <Stage>
        Stir in <Grocery Process="chopped" Quantity="400" Type="can tomatoes" Unit="g" />
        and warm through.
      </Stage>
      <Stage>
        In a <Utensil Name="casserole" /> layer
        <Grocery Quantity="200" Type="lasagna" Unit="g" />,
        the tomato mix and
        <Dairy Process="grated" Quantity="150" Type="cheese" Unit="g" />
        3 times.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl"/> mix
        <Dairy Quantity="300" Type="soured cream" Unit="ml" />
        with
        <Dairy Quantity="100" Type="milk" Unit="ml" />
        and pour on top of the lasagna.
      </Stage>
      <Stage>
        Bake <Temperature Level="medium" /> 50 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Chicken with cabbage and peanuts" Meals="2">
      <Stage>
        <Staple Name="noodles" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" />	fry
        <Grocery Quantity="70" Type="peanuts" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        then remove.
      </Stage>
      <Stage>
        Fry <Vegetable Process="chopped" Quantity="2" Type="red chilli" />
        and
        <Meat Process="sliced" Quantity="300" Type="chicken" Unit="g" />
        in the oil until meat is done.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Process="sliced" Quantity="750" Type="cabbage" Unit="g" />
        and heat through.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Type="black bean sauce" Unit="tbsp" />
        and fry for 2 minutes.
      </Stage>
      <Stage>
        Sprinkle with the peanuts.
      </Stage>
    </Recipe>
    <Recipe Title="Beans and fried eggs in pitta" Meals="1">
      <Stage>
        In a <Utensil Name="pan" /> warm
        <Grocery Quantity="400" Type="baked beans" Unit="g" />.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Dairy Quantity="2" Type="eggs" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
      </Stage>
      <Stage>
        Grill
        <Grocery Quantity="4" Type="pitta bread" />
      </Stage>
      <Stage>
        Serve with <Vegetable Quantity="200" Unit="g" Type="tomatoes" Process="sliced"/>.
      </Stage>
    </Recipe>

    <Recipe Title="Egg curry" Meals="2">
      <Index>Curry</Index>
      <Stage>
        In <Utensil Name="pan" /> boil
        <Dairy Quantity="4" Type="eggs" />
      </Stage>
      <Stage>
        <Staple Name="rice white"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="350" Type="onions" Unit="g" />
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until soft.
      </Stage>
      <Stage>
        Stir in <Check Quantity="1" Type="chilli powder" Unit="tbsp" />
        and
        <Check Quantity="2" Type="mustard seed" Unit="tsp" />
        and fry until mustard seeds start to pop.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="2" Unit="tbsp" Type="flour" />
        and
        <Check Quantity="1" Unit="tbsp" Type="curry powder" />
        until it is all absorbed.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="150" Unit="g" Type="tomato puree"/>,
        <Grocery Quantity="100" Unit="g" Type="creamed coconut" Process="crumbled"/>
        and
        <Water Quantity="400" Unit="ml" Type="water" />
        and warm through.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="400" Unit="g" Type="mange tout" Process="halved"/>
        and the eggs
        and simmer for 10 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Lentil shepherds pie" Meals="2">
      <Index>
        Shepherds Pie<Sub>Lentil</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="pan"/> boil
        <Vegetable Quantity="1000" Unit="g" Type="boiling potatoes" Process="chopped"/>
        then mash.
      </Stage>
      <Stage>
        In a <Utensil Name="wok"/> fry
        <Vegetable Quantity="700" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>,
        <Vegetable Quantity="400" Unit="g" Type="celery" Process="chopped"/>,
        and
        <Vegetable Quantity="200" Unit="g" Type="carrots" Process="sliced"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="oil"/>
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="250" Unit="g" Type="green lentils" />,
        <Check Quantity="3" Unit="tsp" Type="miso" />,
        <Check Quantity="1" Unit="cube" Type="stock" Process="crumbled" />,
        <Grocery Quantity="75" Unit="g" Type="tomato puree" />,
        <Water Quantity="650" Unit="ml" Type="boiling water" />,
        <Check Quantity="1" Unit="tsp" Type="marjoram" />,
        <Check Quantity="1" Unit="tsp" Type="ground cumin" />
        and
        <Check Quantity="2" Unit="tsp" Type="ground coriander" />
        then bring to boil,
        then simmer for 20 minutes.
      </Stage>
      <Stage>
        Take off heat,
        put in a <Utensil Name="casserole"/>,
        cover with the mashed potatoes
        and sprinkle with
        <Grocery Quantity="50" Unit="g" Type="sunflower seeds"/>.
        then bake <Temperature Level="medium"/> for 20 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Chickpea risotto" Meals="2">
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Process="chopped" Quantity="400" Type="onions" Unit="g" />,
        <Vegetable Quantity="1" Type="red chilli" Process="chopped"/>
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="oil"/>
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="1000" Unit="g" Type="sweet potatoes" Process="sliced" />,
        <Grocery Quantity="400" Unit="g" Type="can chick peas" Process ="drained"/>,
        <Grocery Quantity="400" Unit="g" Type="can tomatoes" Process="chopped"/>,
        <Check Quantity="300" Unit="ml" Type="sherry" />,
        <Grocery Quantity="75" Unit="g" Type="raisins" />,
        <Check Quantity="1" Unit="cube" Type="stock" Process="crumbled" />,
        <Grocery Quantity="400" Unit="g" Type="white rice"/>
        and
        <Water Quantity="500" Unit="ml" Type="water" />
        and bring to a boil.
      </Stage>
      <Stage>
        Put in a
        <Utensil Name="casserole"/>,
        cover and bake <Temperature Level="low"/> 20 minutes.
      </Stage>
      <Stage>
        Server with
        <Dairy Quantity="150" Unit="g" Type="parmesan" Process="grated"/>.
      </Stage>
    </Recipe>
    <Recipe Title="Tortellini and parmesan cheese" Meals="1">
      <Stage>
        <Staple Name="tortellini" />
      </Stage>
      <Stage>
        Sprinkle with
        <Grocery Quantity="20" Unit="g" Type="grated parmesan"/>.
      </Stage>
    </Recipe>
  </Menu>

  <Menu Title="Approved by Mags">
    <Recipe Title="Aubergine and peppers" Meals="2">
      <Stage>
        <Staple Name="potatoes" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="sliced"/>
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="150" Unit="g" Type="red pepper" Process="sliced"/>,
        <Vegetable Quantity="150" Unit="g" Type="green pepper" Process="sliced"/>,
        <Vegetable Quantity="400" Unit="g" Type="aubergine" Process="sliced"/>
        and
        <Vegetable Quantity="400" Unit="g" Type="courgette" Process="sliced"/>
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="150" Unit="g" Type="tomato puree" />,
        <Water Quantity="100" Type="cold water" Unit="ml" />
        and
        <Vegetable Quantity="400" Unit="g" Type="tomatoes" Process="chopped"/>,
        and bring to the boil.
      </Stage>
      <Stage>
        Put in a <Utensil Name="casserole" /> and bake <Temperature Level="medium" /> for 40 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Potatoes Russian style" Meals="2">
      <Index>
        Style<Sub>Russian potatoes</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="pan" /> boil
        <Vegetable Quantity="2000" Unit="g" Type="boiling potatoes" Process="cubed"/>
        until not quite done
        then drain.
      </Stage>
      <Stage>
        Stir in
        <Dairy Quantity="600" Unit="g" Type="cottage cheese" />,
        <Dairy Quantity="300" Unit="ml" Type="sour cream" />,
        <Dairy Quantity="150" Unit="g" Type="cheese" Process="grated"/>,
        <Grocery Quantity="50" Unit="g" Type="capers" />,
        and
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>.
        and bake <Temperature Level="medium" /> for 30 minutes.
      </Stage>
      <Stage>
        Grill <Meat Quantity="4" Type="burgers" /> and serve with the potatoes.
      </Stage>
      <Stage>
        Sprinkle with 
        <Vegetable Quantity="50" Unit="g" Type="spring onion" Process="chopped"/>
        to serve.
      </Stage>
    </Recipe>
    <Recipe Title="Lentils and spicy sausage" Meals="2">
      <Stage>
        <Staple Name="rice brown" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        and
        <Meat Quantity="100" Unit="g" Type="bacon" Process="chopped"/>
        in
        <Check Quantity="3" Unit="tbsp" Type="oil" />
        until bacon done.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="250" Unit="g" Type="red lentils" />,
        <Grocery Quantity="800" Unit="g" Type="can tomatoes" />,
        <Meat Quantity="250" Unit="g" Type="chorizo sausage" Process="sliced"/>,
        <Check Quantity="1" Unit="tsp" Type="chilli powder" />
        and
        <Water Quantity="400" Type="cold water" Unit="ml" />
        and simmer for 25 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Pizza and Swedish salad" Meals="1">
      <Index>
        Pizza
      </Index>
      <Stage>
        Bake <Grocery Quantity="2" Type="pizza" />
        <Temperature Level="high" /> 15 minutes.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl"/> mix
        <Vegetable Quantity="250" Type="cabbage" Unit="g" Process="sliced" />,
        <Vegetable Quantity="50" Type="carrots" Unit="g" Process="sliced" />
        and
        <Vegetable Quantity="50" Type="onions" Unit="g" Process="sliced"/>.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Type="sugar" Unit="tbsp" />,
        <Check Quantity="2" Type="wine vinegar" Unit="tbsp" />,
        <Check Quantity="2" Type="oil" Unit="tbsp" />,
        <Check Quantity="1" Type="basil" Unit="pinch"/>,
        <Check Quantity="1" Type="oregano" Unit="pinch"/>,
        <Check Quantity="1" Type="salt" Unit="pinch"/>
        and
        <Check Quantity="1" Type="pepper" Unit="pinch"/>.
      </Stage>
    </Recipe>

    <Recipe Title="Mushroom Stew" Meals="2">
      <Index>
        Stew<Sub>Mushroom</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="375" Unit="g" Type="onions" Process="chopped"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="600" Unit="g" Type="mushrooms" />
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        and cook until mushrooms start to wilt.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="150" Unit="g" Type="pearl barley" />,
        <Check Quantity="4" Unit="tbsp" Type="soy sauce" />,
        <Check Quantity="5" Unit="tbsp" Type="sherry" />,
        <Check Quantity="1" Unit="cube" Type="stock" Process="crumbled" />,
        and
        <Water Quantity="450" Type="cold water" Unit="ml" />
        and simmer for 25 minutes.
      </Stage>
      <Stage>
        <Staple Name="couscous"/>
      </Stage>
    </Recipe>
    <Recipe Title="Beans Caribbean style" Meals="2">
      <Index>
        Style<Sub>Carribbean beans</Sub>
      </Index>
      <Stage>
        <Staple Name="rice white"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="150" Unit="g" Type="green pepper" Process="chopped"/>,
        <Vegetable Quantity="1" Type="red chilli" Process="chopped"/>
        and
        <Vegetable Quantity="3" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="3" Type="oil" Unit="tbsp" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="400" Unit="g" Type="can kidney beans" />
        and
        <Grocery Quantity="400" Unit="g" Type="can chick peas" />
        in their liquid,
        cover and simmer for 5 minutes.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Unit="tsp" Type="dried thyme"/>,
        <Check Quantity="1" Unit="cube" Type="stock" Process="crumbled" />,
        and
        <Grocery Quantity="400" Unit="g" Type="can coconut milk" />
        Cover and simmer for 10 minutes.
      </Stage>
      <Stage>
        Stir in the rice and serve.
      </Stage>
    </Recipe>
    <Recipe Title="Tomatoes Indian style" Meals="2">
      <Index>
        Style<Sub>Indian tomatoes</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> heat
        <Check Quantity="4" Unit="tbsp" Type="oil" />,
        <Check Quantity="0.5" Unit="tsp" Type="asafoetida" />,
        <Check Quantity="0.5" Unit="tsp" Type="turmeric" />,
        and
        <Check Quantity="2" Unit="tsp" Type="cumin seeds" />
        until
        the cumin sizzles.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="1" Type="red chilli" Process="chopped"/>
        and
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="chopped"/>
        and
        fry until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="1000" Unit="g" Type="tomatoes" Process="chopped"/>
        and
        <Vegetable Quantity="400" Unit="g" Type="green beans" />
        simmer for 15 minutes.
      </Stage>
      <Stage>
        Serve with
        <Dairy Quantity="500" Unit="g" Type="plain yoghurt"/>
        and
        <Grocery Quantity="4" Type="naan bread"/>.
      </Stage>
    </Recipe>
    <Recipe Title="Bacon and mushroom pasta" Meals="1">
      <Stage>
        <Staple Name="tagliatelli"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry <Meat Quantity="150" Unit="g" Type="bacon" Process="chopped"/>
        and
        <Vegetable Quantity="200" Unit="g" Type="mushrooms" Process="sliced"/>
        in
        <Check Quantity="1" Unit="tbsp" Type="oil" /> for 5 minutes.
      </Stage>
      <Stage>
        Stir in the pasta (cooked and drained)
        and <Dairy Quantity="100" Unit="g" Type="cheese" Process="grated"/>
        and warm through.
      </Stage>
    </Recipe>
  </Menu>

  <Menu Title="Previously E">
    <Recipe Title="Lentil dhansak" Meals="2">
      <Index>
        Curry<Sub>Lentil dhansak</Sub>
      </Index>
      <Stage>
        <Staple Name="rice brown"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="200" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        and
        <Vegetable Quantity="100" Unit="g" Type="carrots" Process="sliced"/>
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Unit="tbsp" Type="curry powder" />,
        <Check Quantity="1" Unit="tsp" Type="cumin seeds" />,
        <Check Quantity="1" Unit="tsp" Type="mustard seeds" />
        and
        <Check Quantity="5" Unit="cm" Type="cinnamon stick" />
        and fry for 2 minutes.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="400" Unit="g" Type="can tomatoes" Process="chopped"/>,
        <Grocery Quantity="2" Unit="tbsp" Type="cashew nuts" />,
        <Check Quantity="120" Unit="ml" Type="creamed coconut" />,
        <Grocery Quantity="225" Unit="g" Type="red lentils" />
        and
        <Vegetable Quantity="150" Unit="g" Type="red pepper" Process="chopped"/>
        and simmer for 20 minutes.
      </Stage>
      <Stage>
        Add juice of
        <Vegetable Quantity="1" Type="lemon" />
        and warm.
      </Stage>
    </Recipe>
    <Recipe Title="Spaghetti and pasta sauce" Meals="2">
      <Index>
        Spaghetti<Sub>Pasta sauce</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="400" Unit="g" Type="onions" />,
        <Meat Quantity="400" Unit="g" Type="minced meat" />,
        <Vegetable Quantity="3" Unit="cloves" Type="garlic" Process="crushed"/>
        and
        <Vegetable Quantity="100" Unit="g" Type="mushrooms" Process="sliced"/>.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="450" Unit="g" Type="pasta sauce" />,
        and warm through.
      </Stage>
      <Stage>
        <Staple Name="spaghetti"/>
      </Stage>
    </Recipe>
    <Recipe Title="Sausages in Cider" Meals="2">
      <Stage>
        In a <Utensil Name="pan"/> boil
        <Vegetable Quantity="1200" Unit="g" Type="boiling potatoes" Process="chopped"/>
        then mash.
      </Stage>
      <Stage>
        In a <Utensil Name="casserole"/> put
        <Meat Quantity="500" Unit="g" Type="sausages"/>,
        <Vegetable Quantity="250" Unit="g" Type="onions" Process="quartered"/>
        and
        <Vegetable Quantity="2" Type="garlic" Process="crushed"/>
        bake <Temperature Level="medium"/> 15 minutes.
      </Stage>
      <Stage>
        Add <Vegetable Quantity="1" Type="apple" Process="sliced"/>
        and
        <Grocery Quantity="400" Unit="ml" Type="cider"/>
        bake <Temperature Level="medium"/> 20 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Egg and chips" Meals="1">
      <Stage>
        Heat up
        <Grocery Quantity="800" Unit="g" Type="oven ready chips" />.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Dairy Quantity="4" Type="eggs"/>
        in
        <Check Quantity="1" Unit="tbsp" Type="oil" />.
      </Stage>
    </Recipe>

    <Recipe Title="Chicken pilaf" Meals="2">
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Meat Quantity="600" Unit="g" Type="chicken" />,
        <Vegetable Quantity="200" Unit="g" Type="onions" Process="chopped"/>
        and
        <Vegetable Quantity="5" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until chicken done.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Unit="tsp" Type="ground cumin" />,
        <Check Quantity="0.5" Unit="tsp" Type="ground cinnamon" />,
        and
        <Check Quantity="6" Type="cloves" />
        and fry for 2 minutes.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="400" Unit="g" Type="white rice" />,
        <Check Quantity="1" Unit="cube" Type="stock" Process="crumbled" />,
        and
        <Water Quantity="800" Unit="ml" Type="water" />
        and bake, covered <Temperature Level="medium" /> 30 minutes.
      </Stage>
      <Stage>
        Spinkle on
        <Grocery Quantity="100" Unit="g" Type="flaked almonds" />
        before serving.
      </Stage>
    </Recipe>
    <Recipe Title="Vegetable shepherds pie" Meals="2">
      <Index>
        Shepherds Pie<Sub>Vegetable</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="pan" /> boil
        <Vegetable Quantity="1000" Unit="g" Type="boiling potatoes" Process="cubed"/>,
        then mash with
        <Dairy Quantity="50" Unit="g" Type="butter" />
        and
        <Dairy Quantity="4" Unit="tbsp" Type="milk" />.
      </Stage>
      <Stage>
        In a <Utensil Name="casserole" /> mix
        <Vegetable Quantity="300" Unit="g" Type="carrots" Process="sliced"/>,
        <Vegetable Quantity="300" Unit="g" Type="parsnips" Process="sliced"/>,
        <Vegetable Quantity="300" Unit="g" Type="swedes" Process="sliced"/>,
        <Grocery Quantity="400" Unit="g" Type="can borlotti beans" Process="undrained"/>,
        <Grocery Quantity="200" Unit="g" Type="mayonaise" />
        and
        <Grocery Quantity="160" Unit="g" Type="horseraddish sauce" />.
        Spread the mashed potatoes on top,
        sprinkle with
        <Check Quantity="50" Unit="g" Type="sunflower seeds" />
        and
        bake <Temperature Level="medium" /> 60 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Onion pizza" Meals="2">
      <Index>
        Pizza<Sub>Onion</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="wok" /> slowly fry
        <Vegetable Quantity="1000" Unit="g" Type="onions" Process="chopped"/>
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="3" Unit="tbsp" Type="oil" />
        until brown.
      </Stage>
      <Stage>
        Spread on <Grocery Quantity="2" Type="pizza bases" />
        then sprinkle with
        <Grocery Quantity="200" Unit="g" Type="pitted olives" />,
        <Grocery Quantity="100" Unit="g" Type="anchovy fillets" />
        and
        <Check Quantity="1" Unit="tsp" Type="mixed herbs" />.
        Bake <Temperature Level="high" /> 20 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Pasta and pesto" Meals="1">
      <Stage>
        <Staple Name="pasta shapes"/>.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="100" Unit="g" Type="pesto" /> and
        <Grocery Quantity="150" Unit="g" Type="can tuna fish" />.
      </Stage>
    </Recipe>
  </Menu>

  <Menu Title="Winter Solstice">
    <Recipe Title="Vegetable mousaka" Meals="2">
      <Stage>
        In a <Utensil Name="pan" /> boil
        <Vegetable Quantity="500" Unit="g" Type="potatoes" Process="sliced"/>
        until soft,
        then drain.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="400" Unit="g" Type="onions" Process="sliced"/>
        and
        <Vegetable Quantity="1000" Unit="g" Type="aubergine" Process="sliced"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>,
        <Vegetable Quantity="100" Unit="g" Type="mushrooms" Process="sliced"/>,
        <Vegetable Quantity="500" Unit="g" Type="tomatoes" Process="chopped"/>,
        <Grocery Quantity="150" Unit="g" Type="tomato puree" />,
        <Water Quantity="3" Unit="tbsp" Type="water" />
        and
        <Check Quantity="1" Unit="tsp" Type="mixed herbs" />,
        and simmer for 10 minutes.
      </Stage>
      <Stage>
        Put everything in a <Utensil Name="casserole" />
        and bake <Temperature Level="medium" /> 30 minutes.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl" /> beat together
        <Dairy Quantity="2" Type="eggs" />,
        <Dairy Quantity="150" Unit="ml" Type="plain yoghurt" />
        and
        <Dairy Quantity="200" Unit="ml" Type="milk"/>.
        Pour on the <Utensil Name="casserole" />, spinkle with
        <Dairy Quantity="150" Unit="g" Type="cheese" Process="grated"/>
        and bake <Temperature Level="medium" /> 20 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Chickpea and spinach curry" Meals="2">
      <Index>
        Curry<Sub>Chickpea and spinach</Sub>
      </Index>
      <Stage>
        <Staple Name="rice brown"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Check Quantity="2" Unit="tsp" Type="cumin seeds" />
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        for 2 minutes.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>
        and
        <Vegetable Quantity="400" Unit="g" Type="onions" Process="chopped"/>
        and fry until until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="500" Unit="g" Type="spinach" Process="hacked"/>,
        <Vegetable Quantity="500" Unit="g" Type="mushrooms" Process="sliced"/>,
        <Grocery Quantity="400" Unit="g" Type="can chick peas" Process="drained"/>,
        <Check Quantity="2" Unit="tbsp" Type="soy sauce" />
        and
        <Check Quantity="2" Unit="tsp" Type="garam masala" />
        and simmer until spinach wilts.
      </Stage>
    </Recipe>
    <Recipe Title="Courgettes and bacon" Meals="2">
      <Stage>
        <Staple Name="potatoes" />
        then slice.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="400" Unit="g" Type="courgette" Process="sliced"/>,
        <Vegetable Quantity="400" Unit="g" Type="onions" Process="sliced"/>
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>.
        in
        <Check Quantity="2" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Meat Quantity="6" Unit="rashers" Type="bacon" Process="chopped"/>,
        <Dairy Quantity="100" Unit="g" Type="cheese" Process="grated"/>,
        <Dairy Quantity="2" Type="eggs" Process="beaten"/>,
        and
        <Dairy Quantity="4" Unit="tbsp" Type="milk" />.
        Layer in a <Utensil Name="casserole" />
        with the potatoes
        and bake <Temperature Level="medium" /> 35 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Tagliatelli and olive oil" Meals="1">
      <Stage>
        <Staple Name="tagliatelli"/>
        save 150 ml of the water.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>,
        <Vegetable Quantity="200" Unit="g" Type="courgette" Process="sliced"/>,
        <Vegetable Quantity="150" Unit="g" Type="red pepper" Process="sliced"/>,
        <Check Quantity="1"  Type="bay leaf" Process="crumbled"/>
        and
        <Vegetable Quantity="1" Type="red chilli" Process="chopped"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="olive oil" />
        until garlic browns.
      </Stage>
      <Stage>
        Stir in the tagliatelli,
        the water
        and
        <Grocery Quantity="200" Unit="g" Type="olives" />
        and simmer for 3 minutes.
      </Stage>
      <Stage>
        Spinkle with
        <Grocery Quantity="50" Unit="g" Type="grated parmesan"/>.
      </Stage>
    </Recipe>

    <Recipe Title="Chicken curry" Meals="2">
      <Index>
        Curry<Sub>Chicken</Sub>
      </Index>
      <Stage>
        <Staple Name="rice white"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="chopped"/>
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="3" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="2" Unit="tbsp" Type="curry powder" />,
        <Meat Quantity="400" Unit="g" Type="chicken" Process="sliced" />
        and cook until done.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="400" Unit="g" Type="can evaporated milk" />
        and
        bake <Temperature Level="medium" /> 25 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Lentil and vegetable curry" Meals="2">
      <Index>
        Curry<Sub>Lentil and vegetable</Sub>
      </Index>
      <Stage>
        In a <Utensil Name="pan" /> in
        <Water Quantity="450" Unit="ml" Type="water"/>
        cook
        <Grocery Quantity="225" Unit="g" Type="red lentils" />
        until soft.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Check Quantity="1" Unit="tsp" Type="cumin seeds" />,
        <Check Quantity="1" Unit="tsp" Type="mustard seeds" />,
        and
        <Check Quantity="1" Unit="tsp" Type="ground cinnamon" />
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="400" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>,
        <Vegetable Quantity="100" Unit="g" Type="carrots" Process="sliced"/>,
        <Vegetable Quantity="150" Unit="g" Type="red pepper" Process="chopped"/>
        and
        <Grocery Quantity="400" Unit="g" Type="can tomatoes" Process="chopped"/>,
        and simmer for 5 minutes.
      </Stage>
      <Stage>
        Stir in
        the lentils,
        <Check Quantity="100" Unit="g" Type="creamed coconut" />,
        <Check Quantity="1" Unit="tbsp" Type="curry powder" />,
        <Grocery Quantity="2" Unit="tbsp" Type="cashew nuts" />
        and juice of
        <Vegetable Quantity="1" Type="lemon" />.
      </Stage>
      <Stage>
        <Staple Name="couscous"/>
      </Stage>
    </Recipe>
    <Recipe Title="Sweet corn pizza" Meals="2">
      <Index>
        Pizza<Sub>Sweet corn</Sub>
      </Index>
      <Stage>
        On
        <Grocery Quantity="2" Type="pizza bases" />
        spread
        <Grocery Quantity="300" Unit="g" Type="tomato puree" />,
        <Vegetable Quantity="400" Unit="g" Type="tomatoes" Process="chopped"/>,
        <Grocery Quantity="200" Unit="g" Type="can sweetcorn" />,
        <Vegetable Quantity="150" Unit="g" Type="green pepper" Process="chopped"/>,
        <Grocery Quantity="100" Unit="g" Type="anchovy fillets" />,
        <Grocery Quantity="200" Unit="g" Type="pineapple chunks" />,
        <Dairy Quantity="200" Unit="g" Type="cheese" Process="grated"/>
        and
        <Check Quantity="2" Unit="tsp" Type="oregano" />.
      </Stage>
      <Stage>
        Bake <Temperature Level="high" /> 20 minutes or until golden.
      </Stage>
    </Recipe>
    <Recipe Title="Fish fingers and baked beans" Meals="1">
      <Stage>
        In a <Utensil Name="pan" /> heat up
        <Grocery Quantity="400" Unit="g" Type="can baked beans" />.
      </Stage>
      <Stage>
        Grill
        <Grocery Quantity="8" Type="fish fingers" />.
      </Stage>
    </Recipe>
  </Menu>

  <Menu Title="All New">
    <Recipe Title="Tuna Bake" Meals="2">
      <Stage>
        <Staple Name="rice brown" />
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="sliced"/>
        and
        <Vegetable Quantity="1" Type="chillis" Process="chopped" />
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        In a <Utensil Name="casserole"/> mix
        the rice and onions.
        Stir in
        <Grocery Quantity="400" Unit="g" Type="can mushroom soup" />,
        <Grocery Quantity="200" Unit="g" Type="can sweetcorn" Process="drained"/>,
        <Grocery Quantity="200" Unit="g" Type="frozen peas"/>
        and
        <Grocery Quantity="150" Unit="g" Type="can tuna fish" Process="crumbled" />.
        Sprinkle with
        <Dairy Process="grated" Quantity="50" Type="cheese" Unit="g" />.
        and
        bake <Temperature Level="medium" /> 20 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Sausage and Beans" Meals="2">
      <Stage>
        <Staple Name="potatoes" />
      </Stage>
      <Stage>
        In a <Utensil Name="casserole"/> put
        <Meat Quantity="500" Unit="g" Type="sausages"/>
        bake <Temperature Level="medium" /> 20 minutes.
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="350" Unit="g" Type="onions" Process="sliced"/>
        and
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="400" Unit="g" Type="can chick peas" Process ="drained"/>,
        <Grocery Quantity="400" Unit="g" Type="can borlotti beans"  Process ="drained"/>,
        <Grocery Quantity="140" Type="tomato puree" Unit="g"/>
        and
        <Grocery Quantity="400" Unit="g" Type="can tomatoes" Process="chopped"/>
        and warm through.
      </Stage>
      <Stage>
        Pour beans over the sausages and bake <Temperature Level="medium" /> 30 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Red Pepper Pasta" Meals="2">
      <!-- http://allrecipes.co.uk/recipe/2004/farfalle-with-red-pepper-sauce.aspx?o_is=Hub_TopRecipe_5 -->
      <Stage>
        In a <Utensil Name="pan" /> simmer
        <Vegetable Quantity="4" Type="red pepper" Process="chopped" />,
        <Check Quantity="2" Unit="cube" Type="stock" Process="crumbled" />,
        <Check Quantity="2" Unit="tsp" Type="oregano" />
        and
        <Grocery Quantity="1" Type="can tomatoes"/>
        <Water Quantity="400" Unit="ml" />
        for 20 minutes.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="140" Type="tomato puree" Unit="g"/>,
        <Check Quantity="2" Type="wine vinegar" Unit="tbsp" />
        and
        <Check Quantity="2" Type="honey" Unit="tsp" />
        then mash or blend.
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="400" Unit="g" Type="frozen peas"/>
        and warm through.
      </Stage>
      <Stage>
        <Staple Name="pasta shapes" />
      </Stage>
    </Recipe>
    <Recipe Title="Hangover Noodles" Meals="1">
      <!-- http://www.jamieoliver.com/recipes/vegetables-recipes/hungover-noodles/#vOYV0KputHT3r8De.97 -->
      <Stage>
        In a <Utensil Name="pan" />
        put
        <Grocery Quantity="200" Unit="g" Type="noodles"/>,
        <Vegetable Quantity="400" Unit="g" Type="broccoli" Process="flourettes"/>
        <Vegetable Quantity="400" Unit="g" Type="chinese cabbage" Process="sliced"/>
        <Vegetable Quantity="200" Unit="g" Type="mange tout" Process="halved"/>
        pour on <Water Quantity="800" Unit="ml" Type="boiling water"/>,
        cover and simmer for 5 minutes, then drain.
      </Stage>
      <Stage>
        In a <Utensil Name="bowl"/> mix
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>,
        <Vegetable Quantity="3" Unit="cm" Type="ginger" Process="grated"/>,
        <Check Quantity="2" Unit="tbsp" Type="soy sauce" />,
        <Check Quantity="3" Unit="tbsp" Type="sesame oil" /> and
        <Check Quantity="3" Unit="tbsp" Type="wine vinegar" />.
      </Stage>
      <Stage>
        In a <Utensil Name="frying"/> fry
        <Dairy Quantity="2" Type="eggs" />
        in
        <Check Quantity="1" Unit="tbsp" Type="sesame oil" />.
      </Stage>
      <Stage>
        Toss the noodles in the soy sauce dressing.
        Serve with fried eggs on top.
      </Stage>
    </Recipe>

    <Recipe Title="Chicken and Tomato Pasta" Meals="2">
      <Stage>
        In a <Utensil Name="wok"/> fry
        <Vegetable Quantity="300" Unit="g" Type="onions" Process="sliced"/>
        and
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Grocery Process="chopped" Quantity="400" Type="can tomatoes" Unit="g" />,
        <Grocery Quantity="140" Type="tomato puree" Unit="g"/>
        and
        <Grocery Quantity="500" Type="red wine" Unit="ml" />
        and simmer gently for 30 minutes.
      </Stage>
      <Stage>
        In a <Utensil Name="casserole"/> place
        <Meat Process="sliced" Quantity="300" Type="chicken" Unit="g" />
        and
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        then bake <Temperature Level="medium" /> 30 minutes.
      </Stage>
      <Stage>
        <Staple Name="tagliatelli"/>
      </Stage>
      <Stage>
        Add meat to the tomatoes, then pour on the pasta.
      </Stage>
    </Recipe>
    <Recipe Title="Tofu and mangetous stew" Meals="2">
      <Index>
        Stew<Sub>Tofu and mange tous</Sub>
      </Index>
      <Stage>
        <Staple Name="rice brown"/>
      </Stage>
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Grocery Quantity="300" Unit="g" Type="tofu" Process="cubed"/>
        in
        <Check Quantity="7" Unit="tbsp" Type="oil"/>
        until brown, then remove.
      </Stage>
      <Stage>
        Fry <Vegetable Quantity="200" Unit="g" Type="onions" Process="sliced"/> until soft.
      </Stage>
      <Stage>
        Stir in
        <Vegetable Quantity="200" Unit="g" Type="mange tout" Process="halved"/>
        and
        <Vegetable Quantity="150" Unit="g" Type="red pepper" Process="sliced"/>,
        <Vegetable Quantity="200" Unit="g" Type="mushrooms" Process="sliced"/>,
        <Vegetable Quantity="400" Unit="g" Type="cabbage" Process="sliced"/>,
        <Check Quantity="3" Unit="tbsp" Type="sherry" />,
        <Vegetable Quantity="3" Unit="cm" Type="ginger" Process="chopped"/>,
        <Vegetable Quantity="4" Unit="cloves" Type="garlic" Process="crushed"/>
        and
        <Check Quantity="3" Unit="tbsp" Type="soy sauce" />
        and cook until done.
      </Stage>
      <Stage>
        Return the tofu.
      </Stage>
    </Recipe>
    <Recipe Title="Vegetable cous cous" Meals="2">
      <Stage>
        In a <Utensil Name="wok" /> fry
        <Vegetable Quantity="200" Unit="g" Type="onions" Process="chopped"/>,
        <Vegetable Quantity="225" Unit="g" Type="courgette" Process="sliced"/>,
        <Vegetable Quantity="225" Unit="g" Type="green beans" Process="chopped"/>,
        <Vegetable Quantity="225" Unit="g" Type="carrots" Process="chopped"/>,
        <Vegetable Quantity="500" Unit="g" Type="cauliflower" />
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="4" Unit="tbsp" Type="oil" />
        until soft.
      </Stage>
      <Stage>
        Stir in
        <Check Quantity="1" Unit="tsp" Type="ground turmeric" Process=""/>,
        <Check Quantity="1" Unit="tsp" Type="ground cinnamon" Process=""/>,
        <Check Quantity="1" Unit="cube" Type="stock" Process="crumbled"/>,
        <Grocery Quantity="400" Unit="g" Type="can chick peas" Process="drained"/>.
        and
        <Water Quantity="400" Unit="ml" Type="water" />,
        and bring to the boil.
      </Stage>
      <Stage>
        Make a space in the middle and pour in
        <Grocery Quantity="400" Unit="g" Type="cous cous"/>
        in the middle, cover,
        and leave to stand for 10 minutes.
      </Stage>
    </Recipe>
    <Recipe Title="Noodles and sardines" Meals="1">
      <Stage>
        <Staple Name="noodles"/>
      </Stage>
      <Stage>
        Stir in
        <Grocery Quantity="120" Unit="g" Type="can sardines" Process="mashed"/>
        and
        <Check Quantity="1" Unit="tsp" Type="soy sauce" />.
      </Stage>
    </Recipe>
  </Menu>

  <Menu Title="May Bank Holiday">
    <Recipe Title="Chicken Tagine" Meals="2">
      <Stage>
        In a <Utensil Name="wok"/> fry
        <Meat Quantity="400" Unit="g" Type="chicken" Process="chopped"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="olive oil" />
        until done, then remove.
      </Stage>
      <Stage>
        In a <Utensil Name="wok"/> fry
        <Vegetable Quantity="300" Unit="g" Type="onions" Process="chopped"/>
        and
        <Vegetable Quantity="2" Unit="cloves" Type="garlic" Process="crushed"/>
        in
        <Check Quantity="2" Unit="tbsp" Type="olive oil" />
        until golden.
      </Stage>
      <Stage>
        Stir the chicken,
        <Check Quantity="1" Unit="tsp" Type="ground cinnamon" />,
        <Check Quantity="1" Unit="tsp" Type="ground cumin" />,
        <Check Quantity="1" Unit="tsp" Type="ground ginger" />,
        <Grocery Quantity="150" Unit="g" Type="dried apricots" Process="quartered"/>,
        <Vegetable Quantity="1" Type="lemon" Process="juiced"/>,
        <Check Quantity="2" Unit="tbsp" Type="honey" />,
        <Check Process="crumbled" Unit="cube" Quantity="1" Type="stock" />
        and
        <Water Quantity="359" Type="boiling water" Unit="ml" />
      </Stage>
      <Stage>
        <Staple Name="couscous"/>
      </Stage>
      <Stage>
        Serve with
        <Grocery Quantity="100" Unit="g" Type="flaked almonds" Process="toasted"/>
        and
        <Vegetable Type="fresh coriander" Process="chopped"/>.
      </Stage>
    </Recipe>
  </Menu>
